//这是PiQpskCode.v文件的程序清单
module PiQpskCode. (
	rst,clk,din,
	di,dq,df); 
	
	input		rst;   //复位信号，高电平有效
	input		clk;   //FPGA系统时钟：8MHz
	input	 signed [7:0]	din;    //输入数据：8MHz
	output signed [25:0]	di;     //同步后的输出信号(同相支路)
	output signed [25:0]	dq;     //同步后的输出信号(正交支路)
	output signed [25:0]	df;     //环路滤波器输出数据